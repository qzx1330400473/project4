body
    background #efefef
    margin 0
    padding 0

    .header
        height 35px
        background #fff
        line-height 35px
        .icon-back
            position absolute
            left 15px
        .header_title
            display inline-block
            width 100%
            text-align center
            font-weight bold
    .search
        padding 15px
        .input
            border 1px solid #d6d7dc
            background url("../../../public/img/APP/release/nav_search_icon@2x.png") no-repeat 10px 10px #fff
            background-size 5%
            padding-left 30px
            height 30px
            width 200px
            border-radius 5px
        .increase
            float right
            padding-left 30px
            background url("../../../public/img/APP/partent/content_plus_icon@2x.png") no-repeat 5px
            width 75px
            height 30px
            line-height 30px
            background-size 17%
    .edifice
        height 245px
        background #fff
        padding 15px
        color #646464
        font-size 12px
        .title
            font-size 14px
        .table
            margin-left 20px
            td
                height 25px
            .text-color
                color #b4b4b4
                border-bottom 1px solid #b4b4b4

    .resources
        height 610px
        background #fff
        padding 15px
        color #646464
        font-size 12px
        margin-top 20px
        .title
            font-size 14px
        .text
            margin-left 20px
        .kind
            list-style none
            li
                float left
                margin-right 10px
                padding-left 20px
                background url("../../../public/img/APP/partent/content_nor_icon@2x.png") no-repeat 0 2px
                background-size 8px 8px
        .kind1
            list-style none
            li
                float left
                margin-right 10px
                padding-left 20px
                background url("../../../public/img/APP/partent/content_nor_icon@2x.png") no-repeat 0 2px
                background-size 8px 8px
        .input
            margin-top 50px
            padding 10px
            border-top 1px solid #e1e1e1

            .input_box
                margin-top 5px
                width 130px
                height 30px
                border 0
                padding-left 10px
                background #efeff4
                border-radius 5px
            .input_box1
                margin-top 5px
                width 230px
                height 30px
                border 0
                padding-left 10px
                background url("../../../public/img/APP/partent/content_list_icon01@2x.png") no-repeat right 12px #efeff4
                background-size 7%
                border-radius 5px
            .date
                float right
                margin-top 15px
                margin-right 15%
                padding-left 15px
                background url("../../../public/img/APP/partent/content_nor_icon@2x.png") no-repeat 0 2px
                background-size 8px 8px
            .input_box2
                margin-top 5px
                width 230px
                height 30px
                border 0
                padding-left 10px
                background #efeff4
                border-radius 5px
            .invioce
                float right
                margin-top 15px
                margin-right 12%
                padding-left 15px
                background url("../../../public/img/APP/partent/content_nor_icon@2x.png") no-repeat 0 2px
                background-size 8px 8px
            .commission
                float right
                margin-top 15px
                margin-right 27%
                padding-left 15px
                background url("../../../public/img/APP/partent/content_nor_icon@2x.png") no-repeat 0 2px
                background-size 8px 8px
            .input_box3
                margin-top -30px
                float right
                width 110px
                height 30px
                border 0
                padding-left 10px
                background #efeff4
                border-radius 5px
            .textarea_box
                padding-top 10px
                .text1
                    display block
                    float left
                    margin-top 10px
                .textarea
                    float left
                    width 250px
                    height 75px
                    border 0
                    padding-left 10px
                    background #efeff4
                    border-radius 5px
            .img
                width 60px
                height 22px
                margin-top 90px
                padding-left 15px
                padding-top 53px
                background url("../../../public/img/APP/partent/content_plus_icon@2x.png") no-repeat center #d4d4d9
                background-size 24px 24px
    .sure
        height 45px
        background #fff
        margin-top 10px
        padding 4%
        .submit
            background #ea5404
            border-radius 5px
            color #fff
            height 40px
            text-align center
            line-height 40px


