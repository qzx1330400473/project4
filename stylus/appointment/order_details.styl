body
    background #efefef
    margin 0
    padding 0

    .header
        height 35px
        background #fff
        line-height 35px

        .icon-back
            content ''
            position absolute
            left 15px
            display block
            width 20px
            height 36px
            background url("../../../public/img/APP/loading/nav_back_icon@2x.png") no-repeat 0 center
            background-size 50%
        .header_title
            display inline-block
            width 100%
            text-align center
            font-weight bold
    .user
        height 85px
        .detailer
            display block
            height 15px
            padding 10px
            color #c5c5c6
        .information
            height 40px
            background url("../../../public/img/APP/oeder_details/content_img_user@2x.png") no-repeat 15px center #fff
            background-size 11%
            padding-left 70px
            padding-top 10px
            .name
                font-size 14px
                font-weight bold
                display block
            .identity
                font-size 12px
                color #646464
            .line
                position absolute
                right 15%
                margin-top -33px
                height 30px
                width 1px
                background #e1e1e1
            .pic_num
                float right
                margin-top -10px
                width 40px
                height 40px
                background url("../../../public/img/APP/oeder_details/content_icon_call@2x.png") no-repeat
                background-size 50%
    .details_information
        height 85px
        color #646464
        .title
            display block
            height 15px
            padding 10px
            color #c5c5c6
        .information
            height 55px
            background #fff
            padding 10px
            font-size 12px
            .name
                font-size 14px
                font-weight bold
                display block
            .hide
                margin-right 22%
                display block
                float left
                margin-top 5px
            .hide_price
                margin-left 13%
                display block
                float left
                margin-top 5px
    .details_date
        clear both
        height 80px
        color #646464
        .name
            display block
            height 25px
            line-height 40px
            padding 10px
            color #c5c5c6
        .date
            background #fff
            height 25px
            padding 10px
    .detail_status
        height 220px
        .status
            display block
            height 25px
            line-height 40px
            padding 10px
            color #c5c5c6
        .every_status
            height 115px
            background #fff
            padding 10px
            font-size 14px
            color #646464
            .success
                height 35px
                padding-top 5px
                border-bottom 1px solid #e1e1e1
                .week
                    display block
                    font-size 12px
                    margin-left 7px
                .order
                    display block
                    float right
                    margin-top -25px
            .success_last
                height 35px
                padding-top 5px
                .week
                    display block
                    font-size 12px
                    margin-left 7px
                .order
                    display block
                    float right
                    margin-top -25px
    .bottom
        height 50px
        position fixed
        clear both
        bottom 0
        width 100%
        left 0
        right 0
        padding 0
        background #fff
        .bottom_list
            list-style none
            left 0
            .list
                float left
                width 32%
                text-align center
                color #ea5404
                border-right 1px solid #e1e1e1
            .list_last
                float left
                width 32%
                text-align center
                color #ea5404




